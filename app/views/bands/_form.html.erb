<%= simple_form_for @band do |f| %>

  <%= f.input :name %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.input :description %>
  <%= f.input :minimum_price %>
  <%= f.association :tags, as: :check_boxes, label_method: :genre %>
  <%= f.submit "Create Band", class: "btn btn-primary" %>

<% end %>
